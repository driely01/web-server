server {
	root /Users/del-yaag/;
	listen 10.11.6.6:8080;
	server_name hello;

	location / {
		cgi_pass	.sh /bin/bash;
		cgi_pass	.out;
		cgi_pass	.cgi;
		cgi_pass	.php /usr/bin/php;
		cgi_pass	.py /usr/bin/python3;
	}

	location /a.out {
		cgi_pass	.out;
